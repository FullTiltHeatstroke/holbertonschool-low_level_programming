#include <stdio.h>
#include <stdlib.h> 

int main(int argc, char *argv[])
{
  int i = 0;
    for (i = 0; i < argc; i++)
      {
    printf("%s\n", argv[i]);
      }
  
    int j = 1;
    for (j = 0; j < argc; j++)
      {
	int x = strtol(argv[i], NULL, 10); 
	j = j * x;

      }
    printf("%d/n", j); 
    return 0;
}

